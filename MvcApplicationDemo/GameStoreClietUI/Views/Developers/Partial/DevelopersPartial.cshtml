
@{
    ViewBag.Title = "DevelopersPartial";
}

<h2>DevelopersPartial</h2>
@foreach (var item in Model)
{
<div class="container">
    @using (@Html.BeginForm("Delete", "Developers", new { Name = item.Name,id=item.Id }, FormMethod.Post))
    {
        <div class="card">
            <h5 class="card-header text-center">@item.Name</h5>
            <div class="card-body">
                <h5 class="card-title">Special title treatment</h5>
                <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                <div class="text-center">
                    <a href="#" class="btn btn-success mr-2">Edit</a>
                    <input type="submit" value="Delete" />
                </div>
            </div>
        </div>
    }
</div>
    }
